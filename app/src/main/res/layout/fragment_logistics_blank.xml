<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              tools:context=".fragment.LogisticsFragment">

    <!-- TODO: Update blank fragment layout -->
    <include
        android:id="@+id/topbar"
        layout="@layout/topbar2"></include>

    <com.flyco.tablayout.SlidingTabLayout
        android:id="@+id/logFraTablayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/tablayoutHeight"
        app:tl_indicator_color="@color/green"
        app:tl_indicator_height="@dimen/tablayoutIndicatorHeight"
        app:tl_tab_space_equal="true"
        app:tl_textSelectColor="@color/green"
        app:tl_textUnselectColor="#999999"
        app:tl_textsize="@dimen/tablayoutTextsize"
        >

    </com.flyco.tablayout.SlidingTabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/logFraViewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></android.support.v4.view.ViewPager>
</LinearLayout>
