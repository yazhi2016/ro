<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              tools:context="com.xmzlb.iron.fragment.MeFragment">

    <com.flyco.tablayout.SlidingTabLayout
        android:id="@+id/meOrderFraTablayout"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#EEEEEE"
        app:tl_textsize="16sp"
        app:tl_indicator_color="#EEEEEE"
        app:tl_tab_space_equal="true"
        app:tl_textSelectColor="@color/red"
        app:tl_textUnselectColor="#999999"
        ></com.flyco.tablayout.SlidingTabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/meOrderFraViewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></android.support.v4.view.ViewPager>
</LinearLayout>
