<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context="com.xmzlb.iron.activity.MyOrderActivity">

    <include
        layout="@layout/topbar"></include>

    <com.flyco.tablayout.SlidingTabLayout
        android:id="@+id/myoActTablayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/tablayoutHeight"
        app:tl_textsize="@dimen/tablayoutTextsize"
        app:tl_indicator_color="@color/green"
        app:tl_indicator_height="@dimen/tablayoutIndicatorHeight"
        app:tl_tab_space_equal="true"
        app:tl_textSelectColor="@color/green"
        app:tl_textUnselectColor="#999999"
        ></com.flyco.tablayout.SlidingTabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/myoActViewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></android.support.v4.view.ViewPager>


</LinearLayout>
